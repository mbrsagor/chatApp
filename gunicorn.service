[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/home/ubuntu/commerce
ExecStart=/home/ubuntu/commerce/venv/bin/gunicorn --access-logfile - --workers 3 --bind unix:/home/ubuntu/commerce.sock commerce.wsgi:application

[Install]
WantedBy=multi-user.target
